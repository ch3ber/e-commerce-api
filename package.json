{
  "name": "e-commer-api",
  "version": "0.2.0",
  "description": "Crud in PostgreSQL with API in Express.js",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:e2e": "npm run test -- --config jest.e2e.config.ts --forceExit",
    "test:e2e:watch": "npm run test:e2e -- --watch",
    "tsc": "tsc",
    "dev": "ts-node ./src/index",
    "start": "node ./dist/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "predeploy": "bash predeploy.sh",
    "deploy": "npm run predeploy && npm run tsc && resolve-tspaths && npm run migrations:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch3ber/e-commer-api.git"
  },
  "keywords": [
    "express",
    "javascript",
    "node",
    "node-js",
    "api",
    "crud",
    "rest-api"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch3ber/e-commer-api/issues"
  },
  "homepage": "https://github.com/ch3ber/e-commer-api#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-transform-async-to-generator": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@faker-js/faker": "7.6.0",
    "@jest/globals": "29.3.1",
    "@types/bcryptjs": "2.4.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.11.18",
    "@types/nodemailer": "6.4.7",
    "@types/passport": "1.0.11",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.10",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "babel-jest": "29.3.1",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.32.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.3.1",
    "jsdoc": "4.0.0",
    "nodemon": "2.0.20",
    "supertest": "6.3.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "nodemailer": "6.9.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "reflect-metadata": "0.1.13",
    "resolve-tspaths": "0.8.3",
    "sequelize": "6.28.0",
    "sequelize-cli": "6.5.2",
    "sequelize-typescript": "2.1.5",
    "swagger-ui-express": "4.6.0",
    "typescript": "4.9.4",
    "yamljs": "0.3.0"
  }
}
