    "node",
    "node-js",
    "api",
    "crud",
    "rest-api"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch3ber/e-commer-api/issues"
  },
  "homepage": "https://github.com/ch3ber/e-commer-api#readme",
