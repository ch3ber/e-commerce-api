{
  "name": "e-commer-api",
  "version": "0.0.0",
  "description": "Crud in PostgreSQL with API in Express.js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --verbose index.js",
    "start": "node index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch3ber/e-commer-api.git"
  },
  "keywords": [
    "express",
    "javascript",
    "node",
    "node-js",
    "api",
    "crud",
    "rest-api"
  ],
  "author": "Eber Edrey Alejo Berrones <eber4202@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch3ber/e-commer-api/issues"
  },
  "homepage": "https://github.com/ch3ber/e-commer-api#readme",
  "imports": {
    "#config/*.js": "./config/*.js",
    "#db/*.js": "./db/*.js",
    "#libs/*.js": "./libs/*.js",
    "#routes/*.js": "./src/routes/*.js",
    "#middlewares/*.js": "./src/middlewares/*.js",
    "#schemas/*.js": "./src/schemas/*.js",
    "#services/*.js": "./src/services/*.js",
    "#utils/*.js": "./src/utils/*.js"
  },
  "devDependencies": {
    "@babel/core": "7.20.7",
    "@babel/plugin-transform-async-to-generator": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.18.9",
    "@faker-js/faker": "7.6.0",
    "babel-jest": "29.3.1",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-preset-es2015": "6.24.1",
    "eslint": "8.28.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.3.1",
    "jsdoc": "4.0.0",
    "nodemon": "2.0.20"
  },
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "joi": "17.7.0",
    "jsonwebtoken": "9.0.0",
    "nodemailer": "6.9.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.26.0",
    "sequelize-cli": "6.5.2",
    "swagger-ui-express": "4.6.0",
    "yamljs": "0.3.0"
  }
}
