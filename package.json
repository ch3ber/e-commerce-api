    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.3.1",
    "jsdoc": "4.0.0",
    "nodemon": "2.0.20",
    "ts-node": "^10.9.1",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.2"
